<h1>Login</h1>
<div class="col-lg-6">
  <%= form_tag("/auth/create", method: "post") do %>
    <%= label_tag(:email, "Email:") %>
    <%= email_field_tag(:email) %>
    <%= label_tag(:password, "Password :") %>
    <%= password_field_tag(:password) %>
    <%= submit_tag("Auth") %>
  <% end %>
</div>
